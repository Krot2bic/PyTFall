{
  "$schema": "http://json-schema.org/draft-04/schema",
  "additionalProperties": false,
  "properties": {
    "cblock": { "type": "number" },
    "chance": { "type": "number" },
    "desc": { "type": "string" },
    "dismod": { "type": "number" },
    "icon": { "type": "string" },
    "id": { "type": "string" },
    "infinite": { "type": "boolean" },
    "locations": {
      "items": { "$ref": "#/definitions/locations" },
      "type": "array"
    },
    "price": { "type": "number" },
    "slot": { "type": "string" },
    "traits": {
      "additionalProperties": false,
      "properties": {
        "Ane": { "type": "number" },
        "Bokukko": { "type": "number" },
        "Dandere": { "type": "number" },
        "Deredere": { "type": "number" },
        "Imouto": { "type": "number" },
        "Impersonal": { "type": "number" },
        "Kamidere": { "type": "number" },
        "Kuudere": { "type": "number" },
        "Tsundere": { "type": "number" },
        "Yandere": { "type": "number" }
      },
      "type": "object"
    },
    "type": { "type": "string" }
  },
  "required": [
    "cblock",
    "chance",
    "dismod",
    "icon",
    "id",
    "infinite",
    "locations",
    "price",
    "slot",
    "desc"
  ],
  "type": "object"
}
